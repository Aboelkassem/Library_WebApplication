@model IEnumerable<Library.Models.Log>

@foreach (var log in Model)
{
    <a class="dropdown-item d-flex align-items-center description" href="#">
        <div class="mr-3">
            <div class="icon-circle bg-primary">
                <i class="fas fa-file-alt text-white"></i>
            </div>
        </div>
        <div>
            <div class="small text-gray-500">@log.date</div>
            <span class="font-weight-bold">@log.message</span>
        </div>
    </a>
}